pipeline {
    agent any
	    stages{
		    stage('Checkout') {
					steps {
						echo 'hello'
					}
				}
		    stage('Build') {
			        steps {
		                    echo "doing"
		            }
			    }
		    stage('deploy') {
			        steps {
		            echo "done"
		            }
			    }
        }	
}